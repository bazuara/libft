/*   Created: 2019/11/05 10:45:33 by bazuara           #+#    #+#             */
/*   Updated: 2019/11/05 17:37:42 by bazuara          ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

char	*ft_strchr(char *str, int c)
{
	int i;

	i = 0;
	while (str[i] != '\0' && str[i] != c)
		i++;
	if (str[i] == c)
		return (&str[i]);
	return (0);
}
